{
  "name": "portfolio",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:up": "docker compose -f docker-compose.dev.yml up -d",
    "db:down": "docker compose -f docker-compose.dev.yml down",
    "db:logs": "docker compose -f docker-compose.dev.yml logs -f postgres",
    "db:ps": "docker compose -f docker-compose.dev.yml ps",
    "db:restart": "pnpm run db:down && pnpm run db:up"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.12.1",
    "@nuxt/ui": "^4.0.1",
    "@unhead/vue": "^2.0.19",
    "nuxt": "^4.1.3",
    "typescript": "^5.9.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "eslint": "^9.37.0",
    "eslint-plugin-format": "^1.0.2"
  }
}
